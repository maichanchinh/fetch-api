{
  # User recommended
  extends: "eslint:recommended",

  # Global variables: false means that these are read-only
  globals: {
    fetch: false,
    require: false,
    document: true,
    window: true,
    jest: false,
    it: false,
    pit: false,
    describe: false,
    expect: false,
    module: false,
    beforeEach: false,
    afterEach: false
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-undef": 0,
    "no-unused-vars":0,
    "no-debugger":0
  }
}